{
    "benchmarks": [
        {
            "name": "fio",
            "ids": "1-2",
            "mv-params": {
                "global-options": [
                    {
			"name": "required",
			"params": [
			    { "arg": "clocksource", "vals": [ "gettimeofday" ] },
			    { "arg": "ramp_time", "vals": [ "5s" ] },

			    { "arg": "bs", "vals": [ "4K", "512K" ] },
			    { "arg": "unlink", "vals": [ "1" ] },
			    { "arg": "filename", "vals": [ "/tmp/fio.foo" ] },
			    { "arg": "runtime", "vals": [ "10s" ] },
			    { "arg": "time_based", "vals": [ "1" ] },
			    { "arg": "norandommap", "vals": [ "ON" ] },
			    { "arg": "rw", "vals": [ "randrw" ] },
			    { "arg": "ioengine", "vals": [ "sync" ] },
			    { "arg": "size", "vals": [ "10M" ] }
			]
                    }
                ],
                "sets": [
                    {
			"include": "required"
                    }
                ]
            }
        }
    ],
    "tool-params": [
        {
            "tool": "sysstat"
        },
        {
            "tool": "procstat"
        },
        {
            "tool": "kernel",
            "params": [
                {
                    "arg": "subtools",
                    "val": "turbostat"
                }
            ]
        }
    ],
    "tags": {
        "description": "fio-k8s-run-file-json"
    },
    "endpoints": [
        {
            "type": "k8s",
            "controller-ip": "CONTROLLER_IP",
            "host": "CI_ENDPOINT_HOST",
            "user": "CI_ENDPOINT_USER",
            "client": "1-2"
        }
    ],
    "run-params": {
	    "num-samples": 1,
	    "test-order": "s"
    }
}
